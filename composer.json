{
    "name": "statikbe/laravel-nova-mail-editor",
    "description": "Customizable mail templates for Laravel in Nova",
    "keywords": [
        "laravel",
        "nova",
        "statikbe",
        "statik",
        "mail",
        "mails",
        "template",
        "custom"
    ],
    "authors": [
        {
            "name": "Kobe Christiaensen",
            "email": "kobe@statik.be"
        },
        {
            "name": "Niels Dupont",
            "email": "niels@statik.be"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1",
        "laravel/nova": "^3.0",
        "laravel/framework": "~9.0",
        "statikbe/laravel-mail-template-engine": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Statikbe\\NovaMailEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Statikbe\\NovaMailEditor\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
